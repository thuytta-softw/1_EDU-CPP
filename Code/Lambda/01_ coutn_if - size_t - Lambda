#include <iostream>
#include <vector>     // vector
#include <algorithm> // count_if, for_each

using namespace std;

int main()
{
    vector <int> vec = {1,2,3,4,5,6,7,8};

    size_t c;

    c = count_if( vec.begin(), vec.end(),  ); 

    cout<<"Integers that are odd: "<<c<<endl;
    return 0;
}
